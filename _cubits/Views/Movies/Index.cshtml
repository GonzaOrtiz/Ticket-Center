@model List<Movie>;
    <div class="row">
        <div class="col-lg-12">
            <h1>Movie</h1>
            <table class="table table-bordered">
                <thead>
                    <tr>
                        <th>Id</th>
                        <th>Name</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>@item.Id</td>
                            <td>@item.Name</td>
                        </tr>
                    }

                </tbody>

            </table>
        </div>
    </div>